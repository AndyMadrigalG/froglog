{
"manifest_version": 3,
"name": "FrogLog",
"version": "1.0",
"description": "Basic Extension to track and analyze user activity on the browser",
"action": {
    "default_popup": "popup.html",
    "default_icon": "images/Sapillo Pixel.png"
},
"permissions": [
    "tabs", 
    "activeTab",
    "scripting"
],
"background": {
    "service_worker": "background.js"
},
"content_scripts": [
    {
    "matches": ["<all_urls>"],
    "js": ["content.js"]
    }
]
}
